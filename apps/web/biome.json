{
	"$schema": "https://biomejs.dev/schemas/2.3.10/schema.json",
	"root": false,
	"extends": ["@repo/biome-config/base"],
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"jsxQuoteStyle": "double",
			"semicolons": "always",
			"trailingCommas": "all",
			"bracketSameLine": false,
			"bracketSpacing": true,
			"arrowParentheses": "always"
		}
	},
	"files": {
		"includes": ["**", "!src/routeTree.gen.ts"]
	}
}
